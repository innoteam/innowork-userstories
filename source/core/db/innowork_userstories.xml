<?xml version='1.0'?>
<database>

    <table name="innowork_bugs">
        <field  name="id"   type="integer"  notnull="1"/>
        <field  name="ownerid"  type="integer"  notnull="1"/>
        <field  name="projectid"  type="integer"/>
        <field  name="title"    type="text"/>
        <field  name="description"  type="text"/>
        <field  name="steps"  type="text"/>
        <field  name="solution" type="text"/>
        <field  name="statusid" type="integer"/>
        <field  name="priorityid"   type="integer"/>
        <field  name="resolutionid" type="integer"/>
        <field  name="done" type="boolean"/>
        <field  name="creationdate" type="timestamp"/>
        <field  name="trashed"  type="boolean"/>
        <field  name="openedby"  type="integer"/>
        <field  name="assignedto"  type="integer"/>
        <field  name="severityid" type="integer"/>
        <field  name="sourceid"   type="integer"/>
        <key    field="id"  type="unique"/>
    </table>

    <sequence name="innowork_bugs_id_seq"/>

</database>
